      "file_path": "/openfisca_tunisia/model/prelevements_obligatoires/impot_revenu/irpp.py",
      "parameters": [
        "impot_revenu.deductions.assurance_vie.conj_plaf",
        "impot_revenu.deductions.assurance_vie.enf_plaf",
        "impot_revenu.deductions.assurance_vie.plaf"
      ],
      "source_code": "def formula(foyer_fiscal, period, parameters):\n    primes_assurance_vie = foyer_fiscal.members('prime_assurance_vie', period = period)\n    somme_primes_assurance_vie = foyer_fiscal.sum(primes_assurance_vie)\n    marie = foyer_fiscal.declarant_principal('statut_marital', period = period)\n    nb_enf = foyer_fiscal('nb_enf', period = period)\n    P = parameters(period.start).impot_revenu.deductions.assurance_vie\n    deduction = min_(somme_primes_assurance_vie, P.plaf + marie * P.conj_plaf + nb_enf * P.enf_plaf)\n    return deduction\n",
      "start_line_number": 525,
      "stop_line_number": 533,
      "variables": [
        "nb_enf",
        "prime_assurance_vie"
      ]
    }
